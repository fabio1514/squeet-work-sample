{
  "expo": {
    "name": "Squeet",
    "slug": "squeet",
    "scheme": "squeet",
    "version": "1.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/mainIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.invenovate.squeet",
      "buildNumber": "1"
    },
    "android": {
      "package": "com.invenovate.squeet",
      "versionCode": 15
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "apiBase": "http://api.squeet.co:8091/api/",
      "imageBase": "http://api.squeet.co:8091/images/"
    }
  }
}